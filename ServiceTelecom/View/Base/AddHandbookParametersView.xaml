                                  Height="35"
                                  Width="270"
                                  Margin="10,0,10,0"
                                  Style="{StaticResource ComboBoxStyle1}"
                                  ItemsSource="{Binding ModelCollections}"
                                  SelectedIndex="{Binding TheIndexModelChoiceCollection, Mode=TwoWay}"
