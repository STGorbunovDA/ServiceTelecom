                <StackPanel Orientation="Horizontal"
                            VerticalAlignment="Center"
                            HorizontalAlignment="Center">
                    <Button x:Name="btnAddHandbookParametersForModel"
                            Command="{Binding AddRadiostationForDocumentInDataBase}"
                            Content="Добавить"
                            Margin ="10,0,15,0"
                            Style="{StaticResource menuButtonStyleAll}" 
                        HorizontalAlignment="Center"    
                        Cursor="Hand">
                    </Button>
                    <Button x:Name="btnGetHandbookParametersByModelForRadiostationCollection"
                            Command="{Binding GetHandbookParametersByModelForRadiostationCollection}"
                            Content="✔"
                            Margin ="0,0,0,0"
                            Style="{StaticResource menuButtonStyleCheck}" 
                            Cursor="Hand">
                    </Button>
